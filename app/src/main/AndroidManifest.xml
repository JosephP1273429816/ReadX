<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="31">

        <activity
            android:name=".view.login"
            android:screenOrientation="portrait"
            android:exported="true"
            android:theme="@style/AppTheme"
            tools:ignore="LockedOrientationActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".view.QuizActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"
            tools:ignore="LockedOrientationActivity,RedundantLabel">
        </activity>
        <activity
            android:name=".view.soport"
            android:exported="false"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"
            tools:ignore="LockedOrientationActivity,RedundantLabel">
        </activity>
        <activity
            android:name=".view.SettingsActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            tools:ignore="LockedOrientationActivity,RedundantLabel">
        </activity>
        <activity
            android:name=".view.RegisterActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            tools:ignore="LockedOrientationActivity,RedundantLabel"
            >
        </activity>
        <activity
            android:name=".view.ScoreActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"
            tools:ignore="LockedOrientationActivity,RedundantLabel">
        </activity>
        <activity
            android:name=".view.Home"
            android:screenOrientation="portrait"
            android:exported="false"
            android:theme="@style/AppTheme"
            tools:ignore="LockedOrientationActivity">
        </activity>
        <activity
            android:name=".view.QuizOne"
            android:screenOrientation="portrait"
            android:exported="false"
            android:theme="@style/AppTheme"
            tools:ignore="LockedOrientationActivity">
        </activity>
    </application>
</manifest>
